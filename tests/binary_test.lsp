(describe binary-and
    (== (binary-and 0x0a 0x05) 0x00)
    (== (binary-and 0x0a 0x18) 0x08)
    (== (binary-and 0xaa 0xf0) 0xa0))
    
(describe binary-or
    (== (binary-or 0x0a 0x05) 0x0f)
    (== (binary-or 0x0a 0x18) 0x1a)
    (== (binary-or 0xaa 0xf0) 0xfa))
    
(describe binary-not
    (== (binary-not 0x0000000a) 0xfffffff5)
    (== (binary-not 0x0a0a0a0a) 0xf5f5f5f5))
    
(describe left-shift
    (== (left-shift 0x05 1) 0x0a)
    (== (left-shift 0x05 2) 0x14)
    (== (left-shift 0x05 3) 0x28)
    (== (left-shift 0x05 4) 0x50))

(describe right-shift
    (== (right-shift 0x50 1) 0x28)
    (== (right-shift 0x50 2) 0x14)
    (== (right-shift 0x50 3) 0x0a)
    (== (right-shift 0x50 4) 0x05)
    (== (right-shift 0x0f 2) 0x03))
